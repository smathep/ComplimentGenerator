server {
    server_name dev.patricks.link dev.patricksmathers.com alanar.app test.alanar.app;
    location / {
		proxy_pass         "http://localhost:8000";
		proxy_redirect     off;
		proxy_set_header   Host $host;
		proxy_set_header   X-Real-IP $remote_addr;
		fastcgi_read_timeout 300s;
		proxy_read_timeout 300;
	}

    # listen 443 ssl; # managed by Certbot
    # ssl_certificate /etc/letsencrypt/live/dev.patricks.link/fullchain.pem; # managed by Certbot
    # ssl_certificate_key /etc/letsencrypt/live/dev.patricks.link/privkey.pem; # managed by Certbot
    # include /etc/letsencrypt/options-ssl-nginx.conf; # managed by Certbot
    # ssl_dhparam /etc/letsencrypt/ssl-dhparams.pem; # managed by Certbot

}server {
    if ($host = alana.patricks.link) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    if ($host = alana.patricksmathers.com) {
        return 301 https://$host$request_uri;
    } # managed by Certbot


    listen 80;
    server_name alana.patricksmathers.com alana.patricks.link test.alanar.app;
    return 404; # managed by Certbot




}